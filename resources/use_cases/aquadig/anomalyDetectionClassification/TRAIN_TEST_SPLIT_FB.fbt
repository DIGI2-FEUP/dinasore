<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE FBType SYSTEM "http://www.holobloc.com/xml/LibraryElement.dtd">
<FBType Name="TRAIN_TEST_SPLIT_FB" OpcUa="SERVICE">
    <InterfaceList>
        <EventInputs>
            <Event Name="INIT" Type="Event"/>
            <Event Name="RUN" Type="Event" OpcUa="Method">
                <With Var="X_y"/>
                <With Var="test_proportion"/>
            </Event>
        </EventInputs>
        <EventOutputs>
            <Event Name="INIT_O" Type="Event"/>
            <Event Name="RUN_O" Type="Event">
                <With Var="X_train"/>
                <With Var="X_test"/>
                <With Var="y_train"/>
                <With Var="y_test"/>
            </Event>
        </EventOutputs>
        <InputVars>
            <VarDeclaration Name="X_y" Type="STRING" OpcUa="Variable.RUN"/>
            <VarDeclaration Name="test_proportion" Type="REAL" OpcUa="Variable.RUN"/>
        </InputVars>
        <OutputVars>
            <VarDeclaration Name="X_train" Type="STRING" OpcUa="Variable.RUN"/>
            <VarDeclaration Name="X_test" Type="STRING" OpcUa="Variable.RUN"/>
            <VarDeclaration Name="y_train" Type="STRING" OpcUa="Variable.RUN"/>
            <VarDeclaration Name="y_test" Type="STRING" OpcUa="Variable.RUN"/>
        </OutputVars>
    </InterfaceList>
</FBType>